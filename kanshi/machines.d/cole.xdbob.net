profile laptop-base {
	output eDP-1 enable position 0,0 transform normal
	exec /home/xdbob/.bin/mako-output "Lenovo Group Limited 0x4167 Unknown" && notify-send -u low "kanshi: profile base enabled"
}
profile work-triple {
	output eDP-1 enable position 0,0 transform normal
	output "AOC 2790 AMTH69A000708" enable position 1920,0 transform normal
	output "AOC 2790 AMTH69A000338" enable position 3840,0 transform normal
	exec /home/xdbob/.bin/mako-output "Ancor Communications Inc VG248 E4LMQS100257" && notify-send -u low "kanshi: profile work (triple) enabled"
}
